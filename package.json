    "css-loader": "^2.1.1",
    "cz-conventional-changelog": "2.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jest": "24.9.0",
    "jest-haste-map": "^24.6.0",
